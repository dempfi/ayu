{
  "name": "ayu",
  "displayName": "Ayu",
  "description": "A simple theme with bright colors and comes in three versions â€” dark, light and mirage for all day long comfortable work.",
  "version": "1.0.4",
  "publisher": "teabyii",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ayu-theme/vscode-ayu"
  },
  "bugs": {
    "url": "https://github.com/ayu-theme/vscode-ayu/issues",
    "email": "teabyii@gmail.com"
  },
  "engines": {
    "vscode": "^1.13.0",
    "node": ">=22.18.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "icon-theme",
    "theme",
    "ayu"
  ],
  "homepage": "https://github.com/ayu-theme/vscode-ayu",
  "icon": "assets/logo.png",
  "contributes": {
    "themes": [
      {
        "label": "Ayu Dark",
        "uiTheme": "vs-dark",
        "path": "./ayu-dark.json"
      },
      {
        "label": "Ayu Dark Unbordered",
        "uiTheme": "vs-dark",
        "path": "./ayu-dark-unbordered.json"
      },
      {
        "label": "Ayu Light",
        "uiTheme": "vs",
        "path": "./ayu-light.json"
      },
      {
        "label": "Ayu Light Unbordered",
        "uiTheme": "vs",
        "path": "./ayu-light-unbordered.json"
      },
      {
        "label": "Ayu Mirage",
        "uiTheme": "vs-dark",
        "path": "./ayu-mirage.json"
      },
      {
        "label": "Ayu Mirage Unbordered",
        "uiTheme": "vs-dark",
        "path": "./ayu-mirage-unbordered.json"
      }
    ],
    "iconThemes": [
      {
        "id": "ayu",
        "label": "Ayu",
        "path": "./ayu-icons.json"
      }
    ]
  },
  "scripts": {
    "start": "tsx --watch ./src/build.ts",
    "build": "tsx ./src/build.ts && vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ayu": "^9.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vsce": "^1.96.1"
  }
}
